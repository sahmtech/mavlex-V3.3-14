{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aloha/twilio": "^5.0",
        "arcanedev/log-viewer": "*",
        "arcanedev/support": "*",
        "automattic/woocommerce": "^3.1",
        "barryvdh/laravel-dompdf": "^3.1.1",
        "composer/semver": "^3.4",
        "consoletvs/charts": "^6.6",
        "dipeshsukhia/laravel-country-state-city-data": "^2.0",
        "endroid/qr-code": "*",
        "erag/laravel-disposable-email": "^3.2",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.2",
        "knox/pesapal": "^1.0.0",
        "laravel/framework": "^12.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/octane": "*",
        "laravel/passport": "^12.3",
        "laravel/sanctum": "^4.0.2",
        "laravel/socialite": "^5.18",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.5.2",
        "laravellux/html": "^7.1.3",
        "league/flysystem-aws-s3-v3": "3.0",
        "league/fractal": "^0.20.2",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^12.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "mpdf/mpdf": "^8.2",
        "nwidart/laravel-modules": "^9.0",
        "openai-php/laravel": "^0.11.0",
        "phenx/php-svg-lib": "^1.0",
        "pragmarx/google2fa-laravel": "*",
        "pragmarx/recovery": "^0.2.1",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "razorpay/razorpay": "2.*",
        "simplesoftwareio/simple-qrcode": "*",
        "spatie/flysystem-dropbox": "^3.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^9.2.9",
        "spatie/laravel-permission": "^6.9.0",
        "spiral/roadrunner": "^2025.1",
        "spiral/roadrunner-cli": "*",
        "spiral/roadrunner-http": "*",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "^13.3",
        "yajra/laravel-datatables-oracle": "^12.0",
        "ye42/laravel-paystack": "1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "beyondcode/laravel-dump-server": "^2.0.0",
        "driftingly/rector-laravel": "^1.2",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "*",
        "knuckleswtf/scribe": "^5.10",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^3.7",
        "phpunit/phpunit": "^11.2.5",
        "rector/rector": "^1.2",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "files": [
            "app/Http/helpers.php",
            "app/Http/upload_helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "ocramius/package-versions": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
